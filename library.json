{
	"name": "OpenRTK-base-library",
 	"version": "1.0.0",
 	"description": "OpenRTK Base Library",
    "keywords": "sensors, RTK, INS, AI, automotive, navigation",
 	"repository":
    {
       "type": "git",
       "url": "https://github.com/Aceinna/openRTK330-lib.git",
       "branch": "master"
    },
	"build": {
 		"flags": [
			"-I CPU/STM32F469/include",
			"-I CPU/STM32F469/CMSIS",
			"-I CPU/STM32F469/HAL/Inc",
			"-I CPU/STM32F469/HAL/Inc/Legacy",
			"-I CPU/STM32F469/HAL/Src",
			"-I CPU/STM32F469/OS/Inc",
			"-I CPU/STM32F469/",
			"-I Lib/Algorithm/include",
			"-I Lib/Algorithm",
			"-I Lib/Buffer/include",
			"-I Lib/Common/include",
			"-I Lib/GPS",
			"-I Lib/GPS/include",
			"-I Lib/Math/include",
			"-I Lib/Logg/inc",		
			"-I Lib/Cjson/inc",
			"-I Lib/DmaMemcpy/inc",
			"-I Lib/UARTComm",
			"-I Lib/console/inc",
			"-I Lib/",
			"-I LWIP/arch",
			"-I LWIP/lwip-1.4.1/src/include",
			"-I LWIP/lwip-1.4.1/src/include/ipv4",
			"-I LWIP/lwip-1.4.1/src/include/netif",
			"-I LWIP/lwip_app/user/inc",
			"-I LWIP/lwip_app/webserver/inc",
			"-I LWIP/lwip_app/NtripClient/inc" ,
			"-I LWIP/",
			"-I Platform/OpenRTK330L/",
			"-I Platform/OpenRTK330L/Core/include",
			"-I Platform/OpenRTK330L/Board/include",
			"-I Platform/OpenRTK330L/Filter/include",
			"-I Platform/OpenRTK330L/Driver/inc",
			"-I RTOS/FreeRTOS/include",
			"-I RTOS/FreeRTOS/",
			"-I RTOS/FreeRtos_M4/include",
			"-I Sensors/Core",
			"-I Sensors/Core/include",
			"-I Sensors/",	
			"-L Sensors/",	
			"-l libSensors.a"
		],
		"libArchive": false,
        "platforms": "aceinna_imu"
 	}
 }
